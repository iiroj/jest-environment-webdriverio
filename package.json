{
  "name": "jest-environment-webdriverio",
  "version": "1.0.9",
  "description": "Run your tests using Jest and WebdriverIO",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "license": "MIT",
  "homepage": "https://github.com/iiroj/jest-environment-webdriverio",
  "repository": "github:iiroj/jest-environment-webdriverio",
  "bugs": "https://github.com/iiroj/jest-environment-webdriverio/issues",
  "engines": {
    "node": ">= 8.10"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "typecheck": "tsc",
    "test": "jest --projects test",
    "build": "tsc -p tsconfig.build.json",
    "version": "standard-version --no-verify"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && npm run typecheck && npm test && npm run build"
    }
  },
  "dependencies": {
    "webdriverio": "^5.12.5"
  },
  "peerDependencies": {
    "jest-environment-node": "^24.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/chromedriver": "^2.38.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "chromedriver": "^76.0.1",
    "commitlint": "^8.1.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "jest-environment-node": "^24.9.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "keywords": [
    "appium",
    "jest-environment",
    "jest",
    "selenium",
    "webdriver",
    "webdriverio"
  ]
}
