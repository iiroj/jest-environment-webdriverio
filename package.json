{
  "name": "jest-environment-webdriverio",
  "version": "2.0.0",
  "description": "Run your tests using Jest and WebdriverIO",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "license": "MIT",
  "homepage": "https://github.com/iiroj/jest-environment-webdriverio",
  "repository": "github:iiroj/jest-environment-webdriverio",
  "bugs": "https://github.com/iiroj/jest-environment-webdriverio/issues",
  "engines": {
    "node": ">= 10.14.2"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts"
  ],
  "scripts": {
    "lint:base": "eslint --ext .js,.ts",
    "lint": "npm run lint:base -- .",
    "typecheck": "tsc",
    "test": "jest --projects test",
    "build": "tsc -p tsconfig.build.json",
    "version": "standard-version --no-verify"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "npm run lint:base --"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && npm run typecheck && npm run build && npm test"
    }
  },
  "dependencies": {
    "webdriverio": "^6.7.0"
  },
  "peerDependencies": {
    "chromedriver": "^86.0.0",
    "jest-environment-node": "^26.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/chromedriver": "^81.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.5",
    "@types/request": "2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "chromedriver": "^86.0.0",
    "commitlint": "^11.0.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "jest-environment-node": "^26.6.1",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "appium",
    "jest-environment",
    "jest",
    "selenium",
    "webdriver",
    "webdriverio"
  ]
}